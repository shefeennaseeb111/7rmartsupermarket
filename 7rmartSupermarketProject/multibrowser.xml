<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="MultiBrowserTest" parallel="tests" thread-count="5">

	<test name="chrometest">
		<parameter name="browzer" value="chrome"></parameter>
		<parameter name="username" value="admin"></parameter>
		<parameter name="password" value="admin"></parameter>
		<classes>
			<class name="testScript.Login_Pagetest">
				<methods>
					<include
						name="verifyTheuserisAbleToLoginWithInvalidUsernameAndValidPassword" />

				</methods>
			</class>
		</classes>


	</test> 
	<test name="edgetest">
		<parameter name="browzer" value="edge"></parameter>
		<parameter name="username" value="admin"></parameter>
		<parameter name="password" value="admin"></parameter>
		<classes>
			<class name="testScript.Login_Pagetest">
				<methods>
					<include
						name="verifyTheuserisAbleToLoginWithInvalidUsernameAndValidPassword" />

				</methods>
			</class>
		</classes>


	</test> 
</suite> 

